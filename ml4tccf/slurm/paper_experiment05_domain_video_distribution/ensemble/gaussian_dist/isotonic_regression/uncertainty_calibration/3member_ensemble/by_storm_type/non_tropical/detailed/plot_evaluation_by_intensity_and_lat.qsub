--max_wind_cutoffs_kt 50 80 \
--tc_center_latitude_cutoffs_deg_n 15 25 35 45 \
--input_max_wind_based_eval_file_names "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=0.0-50.0_tc_center_latitude_deg_n=-inf-15.0/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=0.0-50.0_tc_center_latitude_deg_n=15.0-25.0/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=0.0-50.0_tc_center_latitude_deg_n=25.0-35.0/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=0.0-50.0_tc_center_latitude_deg_n=35.0-45.0/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=0.0-50.0_tc_center_latitude_deg_n=45.0-inf/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=0.0-50.0_tc_center_latitude_deg_n=all/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=50.0-80.0_tc_center_latitude_deg_n=-inf-15.0/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=50.0-80.0_tc_center_latitude_deg_n=15.0-25.0/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tropical_by_intensity_cutoffs=50-80/max_wind_kt=50.0-80.0_tc_center_latitude_deg_n=25.0-35.0/evaluation.nc" "${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing/storm-type=non_tr