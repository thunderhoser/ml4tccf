--input_prediction_file_pattern="${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing_by_intensity_cutoffs=64-83/${wind_cutoff_string}/predictions*.nc" \
--num_bootstrap_reps=1 \
