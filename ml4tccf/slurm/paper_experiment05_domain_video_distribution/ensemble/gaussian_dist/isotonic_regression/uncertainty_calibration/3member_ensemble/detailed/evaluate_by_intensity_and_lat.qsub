wind_cutoff_string=${WIND_CUTOFF_STRINGS[$SLURM_ARRAY_TASK_ID]}

python3 -u "${CODE_DIR_NAME}/evaluate_neural_net.py" \
--input_prediction_file_pattern="${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing_by_intensity_cutoffs=50-80/${wind_cutoff_string}/predictions*.nc" \
--num_bootstrap_reps=1 \
--num_xy_offset_bins=100 \
--xy_offset_limits_metres -100000 100000 \
--num_offset_distance_bins=50 \
--offset_distance_limits_metres 0 100000 \
--num_offset_direction_bins=36 \
--output_file_name="${model_dir_name}/isotonic_regression_gaussian_dist/uncertainty_calibration/testing_by_intensity_cutoffs=50-80/${wind_cutoff_string}/evaluation.nc"

WIND_CUTOFF_STRINGS=("max_wind_kt=0.0-64.0_tc_center_latitude_deg_n=-inf-15.0" "max_wind_kt=0.0-64.0_tc_center_latitude_deg_n=15.0-25.0" "max_wind_kt=0.0-64.0_tc_center_latitude_deg_n=25.0-35.0" "max_wind_kt=0.0-64.0_tc_center_latitude_deg_n=35.0-45.0" "max_wind_kt=0.0-64.0_tc_center_latitude_deg_n=45.0-inf" "max_wind_kt=0.0-64.0_tc_center_latitude_deg_n=all" "max_wind_kt=64.0-83.0_tc_center_latitude_deg_n=-inf-15.0" "max_wind_kt=64.0-83.0_tc_center_latitude_deg_n=15.0-25.0" "max_wind_kt=64.0-83.0_tc_center_latitude_deg_n=25.0-35.0" "max_wind_kt=64.0-83.0_tc_center_latitude_deg_n=35.0-45.0" "max_wind_kt=64.0-83.0_tc_center_latitude_deg_n=45.0-inf" "max_wind_kt=64.0-83.0_tc_center_latitude_deg_n=all" "max_wind_kt=83.0-inf_tc_center_latitude_deg_n=-inf-15.0" "max_wind_kt=83.0-inf_tc_center_latitude_deg_n=15.0-25.0" "max_wind_kt=83.0-inf_tc_center_latitude_deg_n=25.0-35.0" "max_wind_kt=83.0-inf_tc_center_latitude_deg_n=35.0-45.0" "max_wind_kt=83.0-inf_tc_center_latitude_deg_n=45.0-inf" "max_wind_kt=83.0-inf_tc_center_latitude_deg_n=all" "max_wind_kt=all_tc_center_latitude_deg_n=-inf-15.0" "max_wind_kt=all_tc_center_latitude_deg_n=15.0-25.0" "max_wind_kt=all_tc_center_latitude_deg_n=25.0-35.0" "max_wind_kt=all_tc_center_latitude_deg_n=35.0-45.0" "max_wind_kt=all_tc_center_latitude_deg_n=45.0-inf" "max_wind_kt=all_tc_center_latitude_deg_n=all")
wind_cutoff_string=${WIND_CUTOFF_STRINGS[$SLURM_ARRAY_TASK_ID]}

