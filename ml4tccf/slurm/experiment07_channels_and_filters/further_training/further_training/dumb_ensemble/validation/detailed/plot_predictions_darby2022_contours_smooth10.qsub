#!/bin/tcsh

#SBATCH --job-name="plot_predictions_darby2022_contours_smooth10"
#SBATCH --partition="fge"
#SBATCH --account="rda-ghpcs"
#SBATCH --qos="batch"
#SBATCH --ntasks=1
#SBATCH --nodes=1
#SBATCH --cpus-per-task=1
##SBATCH --nodes=1
##SBATCH --ntasks=1
##SBATCH --cpus-per-task=1
##SBATCH --ntasks-per-node=1
#SBATCH --time=01:00:00
#SBATCH --exclude=h28n12
##SBATCH --exclude=h31n03,h28n12
##SBATCH --exclude=h29n01,h26n05,h28n06,h29n07,h30n03,h30n15
#SBATCH --mail-type=BEGIN,END,FAIL
#SBATCH --mail-user=ryan.lagerquist@noaa.gov
#SBATCH --output=plot_predictions_darby2022_contours_smooth10_%A.out

module load cuda/10.1
source /scratch2/BMC/gsd-hpcs/Jebb.Q.Stewart/conda3.7/etc/profile.d/conda.csh
conda activate base

set CODE_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4tccf_standalone/ml4tccf"
set TOP_MODEL_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4tccf_models/experiment07_channels_and_filters/further_training/further_training"
set SATELLITE_DIR_NAME="/scratch1/RDARCH/rda-ghpcs/Ryan.Lagerquist/ml4tccf_project/satellite_data/processed/normalized_params_from_2017-2020/simplified_all_wavelengths"

set CYCLONE_ID_STRINGS=("2022EP04")
set i=1

while ($i <= ${#CYCLONE_ID_STRINGS})
    set prediction_file_name="${TOP_MODEL_DIR_NAME}/dumb_ensemble/validation/predictions_${CYCLONE_ID_STRINGS[$i]}.nc"

    python3 -u "${CODE_DIR_NAME}/plot_predictions.py" \
    --input_prediction_file_name="${prediction_file_name}" \
    --input_satellite_dir_name="${SATELLITE_DIR_NAME}" \
    --are_data_normalized=1 \
    --plot_one_lag_time=1 \
    --convert_points_to_line_contours=1 \
    --line_contour_smoothing_radius_px=10 \
    --output_dir_name="${TOP_MODEL_DIR_NAME}/dumb_ensemble/validation/predictions_${CYCLONE_ID_STRINGS[$i]}/contours_smooth10"

    @ i = $i + 1
end
